nextflow_process {

  name "Test process xy"

  script "./copy.nf"
  process "COPY"

  test("Should fail because no valid gz file") {

    when {
      process {
        """
        input[0] = file("${moduleTestDir}/invalid.txt.gz")
        """
      }
    }

    then {
      assert process.success
      assert snapshot(process.out.gzip).match()
    }

  }

}
