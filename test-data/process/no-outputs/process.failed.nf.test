nextflow_process {

  name "Test process xy"

  script "test-data/process/no-outputs/process.nf"
  process "TEST_PROCESS"

  test("Should create 5 files") {

    when {
      params {
        outdir = "seb7"
      }
      process {
        """
        input[0] = Channel.of(1..5)
        input[1] = "test"
        """
      }
    }

    then {
      assert process.success
      assert process.out.mychannel
    }

  }

}
